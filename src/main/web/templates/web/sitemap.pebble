<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
   <url>
      <loc>https://www.hectorlopezfernandez.com/index.page</loc>
      <changefreq>daily</changefreq>
   </url>
{% for post in posts %}
   <url>
      <loc>https://www.hectorlopezfernandez.com/archive/{{post.year}}/{{post.month|numberformat("00")}}/{{post.titleUrl}}</loc>
      <lastmod>{{post.lastModificationDate | joda("yyyy-MM-dd")}}</lastmod>
      <changefreq>yearly</changefreq>
   </url>
{% endfor %}
{% for page in pages %}
   <url>
      <loc>https://www.hectorlopezfernandez.com/pages/{{page.titleUrl}}</loc>
      <lastmod>{{page.lastModificationDate | joda("yyyy-MM-dd")}}</lastmod>
      <changefreq>yearly</changefreq>
   </url>
{% endfor %}
</urlset>